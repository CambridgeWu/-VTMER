<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <!--servlet配置 -->
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.cambridge.StuManager.servlet.Loginservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.cambridge.StuManager.servlet.Registerservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>confirmservlet</servlet-name>
        <servlet-class>com.cambridge.StuManager.servlet.Adminservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>courseservlet</servlet-name>
        <servlet-class>com.cambridge.StuManager.servlet.Courseservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>teacherservlet</servlet-name>
        <servlet-class>com.cambridge.StuManager.servlet.Teacherservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>studentservlet</servlet-name>
        <servlet-class>com.cambridge.StuManager.servlet.Studentservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>modifypasswordservlet</servlet-name>
        <servlet-class>com.cambridge.StuManager.servlet.ModifyPasswordservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>examservlet</servlet-name>
        <servlet-class>com.cambridge.StuManager.servlet.Examservlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Loginservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/Registerservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>confirmservlet</servlet-name>
        <url-pattern>/Adminservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>courseservlet</servlet-name>
        <url-pattern>/Courseservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>teacherservlet</servlet-name>
        <url-pattern>/Teacherservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>studentservlet</servlet-name>
        <url-pattern>/Studentservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>modifypasswordservlet</servlet-name>
        <url-pattern>/ModifyPasswordservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>examservlet</servlet-name>
        <url-pattern>/Examservlet</url-pattern>
    </servlet-mapping>
</web-app>